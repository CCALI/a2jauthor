<can-import from="caja/author/templates/list/item/"/>
<can-import from="caja/author/templates/list/list.less!"/>

<ul>
  {{#each displayList}}
    <li {{#if isDraggable}}draggable="true"{{/if}}>
      <templates-list-item
        template:from="'{.}'"
        is-draggable:from='"{isDraggable}"'
        transition-time:from='"{itemTransitionTime}"'>
      </templates-list-item>
    </li>
  {{/each}}
</ul>
