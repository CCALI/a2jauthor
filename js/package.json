{
  "name": "a2j-author-viewer-gui",
  "version": "0.0.1",
  "description": "Authoring and viewer App GUI.",
  "main": "author/main",
  "scripts": {
    "start": "node server",
    "test": "grunt test"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "bit-tabs": "0.2.0",
    "blueimp-file-upload": "^9.10.1",
    "bootstrap": "^3.3.5",
    "can": "^2.3.0-beta.2",
    "chai": "^3.0.0",
    "documentjs": "0.3.0-pre.2",
    "express": "^4.13.0",
    "funcunit": "^3.0.0",
    "grunt": "^0.4.5",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-copy": "^0.8.0",
    "grunt-contrib-less": "^1.0.1",
    "jquery": "^2.1.4",
    "jquery-ui": "git://github.com/jquery/jquery-ui.git#1.11.4",
    "lodash-amd": "^3.10.0",
    "moment": "^2.10.3",
    "steal": "^0.11.0-pre.3",
    "steal-mocha": "0.0.3",
    "steal-tools": "^0.11.0-pre.9",
    "testee": "^0.2.0",
		"grunt-contrib-requirejs": "0.4.4",
		"grunt-jsbeautifier": "0.2.7",
		"connect": "3.2.0",
		"serve-static": "1.6.4",
		"serve-index": "1.4.0"
  },
  "system": {
    "transpiler": "babel",
    "npmIgnore": [
      "testee",
      "documentjs",
      "grunt-contrib-less"
    ],
    "configDependencies": [
      "author/utils/load-fixtures"
    ],
    "bundle": [
      "author/app-template",
      "author/src/src"
    ],
    "map": {
      "chai": "chai/chai",
      "lodash": "lodash-amd/modern",
      "assert": "author/test/assert",
      "jquery.ui.widget": "jquery-ui/ui/widget"
    },
    "meta": {
      "viewer/A2J_Types": {
        "format": "global"
      },
      "viewer/jquery.ui.combobox": {
        "deps": [
          "jquery-ui/ui/widget"
        ]
      },
      "viewer/jquery.ui.traggable": {
        "deps": [
          "jquery-ui/ui/mouse",
          "jquery-ui/ui/widget"
        ]
      },
      "viewer/A2J_AnswerFile": {
        "format": "global",
        "deps": [
          "viewer/A2J_Types"
        ]
      },
      "author/src/A2J_Tabs": {
        "format": "global",
        "deps": [
          "jquery-ui/ui/sortable"
        ]
      },
      "author/src/TextStatistics": {
        "format": "global"
      },
      "author/src/A2J_AuthorApp": {
        "format": "global",
        "deps": [
          "jquery-ui/ui/button",
          "jquery-ui/ui/autocomplete",
          "jquery-ui/ui/draggable",
          "jquery-ui/ui/resizable",
          "jquery-ui/ui/dialog",
          "jquery-ui/ui/tooltip",
          "blueimp-file-upload/js/jquery.fileupload"
        ]
      },
      "blueimp-file-upload/js/jquery.fileupload": {
        "deps": [
          "jquery"
        ]
      }
    }
  }
}
