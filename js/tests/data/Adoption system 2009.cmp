		<hd:computation name="Set up parent name variables CO" resultType="text">
			<hd:script>CLEAR Parent is mother of MC
CLEAR Parent is father of

REPEAT Children
	IF (NOT Petitioner related to any child TF[1] OR NOT (Petitioner 1 relationship to child MC = &quot;Mother&quot;)) 
	AND (Number of petitioners MC = &quot;1&quot; OR NOT Petitioner related to any child TF[2] OR NOT (Petitioner 2 relationship to child MC = &quot;Mother&quot;))
		ADD Child full name-CO TO Parent is mother of MC
	END IF
	IF (Child has known legal father = &quot;Yes&quot; OR Child has biological father or fathers = &quot;Yes&quot;) AND  
	(NOT Petitioner related to any child TF[1] OR NOT (Petitioner 1 relationship to child MC = &quot;Father&quot;)) 
	AND (Number of petitioners MC = &quot;1&quot; OR NOT Petitioner related to any child TF[2] OR NOT (Petitioner 2 relationship to child MC = &quot;Father&quot;))
