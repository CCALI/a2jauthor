		<hd:computation name="Set up shadow repeats CO" resultType="text">
			<hd:script>// set up a repeat to be used in table of children in affidavit for investigator
ERASE Shadow Children  // hidden subdialog of Petitioner
REPEAT Petitioner
	SET Repeat counter TO 1
	WHILE ANSWERED(Child First Name-TE[Repeat counter])
		SET Shadow child name TE[COUNTER, Repeat counter] TO This child full name CO
		SET Shadow child age NU[COUNTER, Repeat counter] TO AGE(Child Birthdate-DA[Repeat counter])
		SET Shadow child gender MC[COUNTER, Repeat counter] TO Child gender MC[Repeat counter]
		IF COUNTER = 1
			SET Shadow petitioner relationship to child MC[COUNTER, Repeat counter] TO Petitioner 1 relationship to child MC[Repeat counter]
		END IF
		IF COUNTER = 2
			SET Shadow petitioner relationship to child MC[COUNTER, Repeat counter] TO Petitioner 2 relationship to child MC[Repeat counter]
		END IF
		INCREMENT Repeat counter
	END WHILE
END REPEAT

// Set up a repeat that will be used for the consent, which needs to repeat for each child and each parent
ERASE Child Parent Match // This dialog holds one repeat for each pair of matching parents and children
SET Repeat counter TO 1
REPEAT Parents
	SET MC counter NU TO 1
	IF Parent gender MC = &quot;Male&quot;
		WHILE SELECTION( Parent is father of, MC counter NU) != &quot;&quot;
			SET Child name match TE[Repeat counter] TO SELECTION( Parent is father of, MC counter NU) 
			SET Parent name match TE[Repeat counter] TO Parent full name CO
			SET Parent type match MC[Repeat counter] TO &quot;Father&quot;
			INCREMENT MC counter NU
			INCREMENT Repeat counter
		END WHILE
	END IF
	IF Parent gender MC = &quot;Female&quot;
		WHILE SELECTION( Parent is mother of MC, MC counter NU) != &quot;&quot;
			SET Child name match TE[Repeat counter] TO SELECTION( Parent is mother of MC, MC counter NU) 
			SET Parent name match TE[Repeat counter] TO Parent full name CO
			SET Parent type match MC[Repeat counter] TO &quot;Mother&quot;
			INCREMENT MC counter NU
			INCREMENT Repeat counter
		END WHILE
	END IF
END REPEAT

// Added 4/10 to accomodate multiple putative fathers for same child
ERASE Shadow Fathers // Hidden subdialog of children

REPEAT Children
	SET Repeat counter TO 1
	SET Child counter TO 1
	WHILE ANSWERED(Child name match TE[Repeat counter])
		IF Child name match TE[Repeat counter] = Child full name-CO
			AND Parent type match MC[Repeat counter] = &quot;Father&quot;
			SET Shadow father name TE[COUNTER, Child counter] TO Parent name match TE[Repeat counter]
			INCREMENT Child counter
		END IF
		SET Number of fathers this child NU TO COUNT(Shadow Fathers)
		INCREMENT Repeat counter
	END WHILE
END REPEAT


// Added 4/10 to accomodate multiple putative fathers for same child
// Create hidden subdialogs for various types of children of fathers
ERASE Putative Children
ERASE  Biological Children
ERASE Legal Children

DEBUG
REPEAT Parents
	IF Parent gender MC = &quot;Male&quot;
		SET Putative children counter TO 0
		SET Biological children counter TO 0
		SET Legal children counter TO 0
		SET Repeat counter 2 TO 1
		WHILE SELECTION(Parent is father of , Repeat counter 2) != &quot;&quot;
			SET Repeat counter TO 1
			WHILE ANSWERED( Child Last Name-TE[Repeat counter])
				IF This child full name CO =  SELECTION(Parent is father of , Repeat counter 2) 
					IF Number of fathers this child NU[Repeat counter] &gt; 1
						INCREMENT Putative children counter
						SET Putative child name TE[COUNTER, Putative children counter] TO SELECTION(Parent is father of, Repeat counter 2)
					ELSE IF Child has known legal father[Repeat counter]
						INCREMENT Legal children counter 
						SET Legal child name TE[COUNTER, Legal children counter] TO SELECTION(Parent is father of, Repeat counter 2)
					ELSE
						INCREMENT Biological children counter
						SET Biological child name TE[COUNTER, Biological children counter] TO SELECTION(Parent is father of, Repeat counter 2)
					END IF
				END IF
				INCREMENT Repeat counter
			END WHILE
			INCREMENT Repeat counter 2
		END WHILE
	END IF
END REPEAT

ASK Parents</hd:script>
