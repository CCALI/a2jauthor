<can-import from="viewer/mobile/" />
<can-import from="viewer/desktop/" />
<can-import from="viewer/styles.less!" />

{{#if isMobile}}
  <a2j-mobile-viewer
    lang="{lang}"
    logic="{logic}"
    r-state="{rState}"
    p-state="{pState}"
    m-state="{mState}"
    interview="{interview}" />
{{else}}
  <a2j-desktop-viewer
    lang="{lang}"
    logic="{logic}"
    r-state="{rState}"
    p-state="{pState}"
    m-state="{mState}"
    interview="{interview}" />
{{/if}}
