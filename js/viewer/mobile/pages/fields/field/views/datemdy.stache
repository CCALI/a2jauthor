<div>
	<div class="form-group {{#if hasError}}has-error{{/if}}">
		<label class="control-label" for="{{label}}">{{{eval label}}}</label>
		<input id="{{label}}" class="form-control" type="date" min="{{dateformat min 'YYYY-MM-DD'}}" max="{{dateformat max 'YYYY-MM-DD'}}" value="{{field._answer.values}}" can-change="update"/>
	</div>

	{{#prompt}}
	<div class="alert alert-danger" role="alert">
		{{{eval field.invalidPrompt}}}
	</div>
	{{/prompt}}
<div>