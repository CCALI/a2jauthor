<form method="POST"
      target="_parent"
      accept-charset="UTF-8"
      class="post-answers-form"
      action="{{scope/pState.setDataURL}}">

  <input type="hidden" name="AnswerKey" el:value:from="scope/answersANX" />

  <button type="submit"
          on:el:click="scope/navigate()"
          class="btn btn-default btn-navigate">
    {{scope/getButtonLabel(scope/label)}}
  </button>
</form>
