<form method="POST"
      target="_parent"
      accept-charset="UTF-8"
      class="post-answers-form"
      action="{{pState.setDataURL}}">

  <input type="hidden" name="AnswerKey" el:value:from="answersANX" />

  <button type="submit"
          on:el:click="navigate"
          class="btn btn-default btn-navigate">
    {{getButtonLabel(label)}}
  </button>
</form>
