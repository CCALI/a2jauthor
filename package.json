{
  "name": "a2jauthor",
  "version": "8.0.4",
  "description": "A2J Authoring App GUI.",
  "main": "a2jauthor/app",
  "scripts": {
    "lint": "standard --fix --env mocha",
    "test": "npm run lint && testee --reporter Spec --browsers firefox test/index.html",
    "build": "grunt build --gruntfile=Gruntfile.js",
    "deploy": "npm i && npm run build && mv index.html index.dev.html && mv index.production.html index.html",
    "build-docs": "grunt documentjs",
    "serve-docs": "grunt documentjs-lite && http-server docs"
  },
  "author": {
    "name": "Center for Computer-Assisted Legal Instruction (CALI)",
    "email": "webmaster@a2jauthor.org",
    "url": "https://www.a2jauthor.org"
  },
  "license": "GNU AGPL v3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/CCALI/a2jauthor"
  },
  "standard": {
    "globals": [
      "$",
      "jQuery",
      "can",
      "mocha"
    ],
    "ignore": [
      "legacy/**",
      "src/models/fixtures/**",
      "build.production.html.js",
      "/ckeditor/",
      "archive",
      "styles/documentjs-theme/**"
    ],
    "env": [
      "mocha"
    ]
  },
  "steal": {
    "plugins": [
      "done-css",
      "steal-less",
      "steal-stache"
    ],
    "npmIgnore": [
      "testee",
      "documentjs",
      "grunt-contrib-less"
    ],
    "configDependencies": [
      "src/utils/set-global-for-lodash"
    ],
    "map": {
      "cuid": "cuid/dist/cuid",
      "jquery-ui/widget": "jquery-ui/ui/widget",
      "localforage": "localforage/dist/localforage.nopromises",
      "chai": "chai/chai"
    },
    "meta": {
      "a2jauthor/legacy/A2J_Tabs": {
        "format": "global",
        "eval": "script",
        "deps": [
          "jquery-ui/ui/widgets/sortable",
          "a2jauthor/ckeditor/ckeditor"
        ]
      },
      "a2jauthor/legacy/A2J_AuthorApp": {
        "format": "global",
        "eval": "script",
        "deps": [
          "jquery-ui/ui/widgets/button",
          "jquery-ui/ui/widgets/autocomplete",
          "jquery-ui/ui/widgets/draggable",
          "jquery-ui/ui/widgets/resizable",
          "jquery-ui/ui/widgets/dialog",
          "jquery-ui/ui/widgets/tooltip"
        ]
      },
      "a2jauthor/legacy/A2J_Clauses": {
        "format": "global",
        "eval": "script"
      },
      "a2jauthor/legacy/A2J_Reports": {
        "format": "global",
        "eval": "script",
        "deps": [
          "a2jauthor/legacy/viewer/A2J_Logic"
        ]
      },
      "a2jauthor/legacy/A2J_Mapper": {
        "format": "global",
        "eval": "script"
      },
      "a2jauthor/legacy/A2J_Pages": {
        "format": "global",
        "eval": "script",
        "deps": [
          "a2jauthor/ckeditor/ckeditor"
        ]
      },
      "a2jauthor/legacy/A2J_Guides": {
        "format": "global",
        "eval": "script"
      },
      "a2jauthor/legacy/A2J_Debug": {
        "format": "global",
        "eval": "script"
      },
      "a2jauthor/legacy/viewer/A2J_Types": {
        "format": "global",
        "eval": "script"
      },
      "a2jauthor/legacy/viewer/A2J_Logic": {
        "format": "global",
        "eval": "script"
      },
      "a2jauthor/legacy/viewer/A2J_ParserConvert": {
        "format": "global",
        "eval": "script"
      },
      "a2jauthor/legacy/viewer/A2J_Parser": {
        "format": "global",
        "eval": "script"
      },
      "a2jauthor/legacy/viewer/A2J_Viewer": {
        "format": "global",
        "eval": "script"
      },
      "a2jauthor/legacy/viewer/A2J_Prefs": {
        "format": "global",
        "eval": "script"
      },
      "a2jauthor/legacy/viewer/A2J_Languages": {
        "format": "global",
        "eval": "script"
      },
      "a2jauthor/legacy/viewer/A2J_MobileFile": {
        "format": "global",
        "eval": "script"
      },
      "a2jauthor/legacy/viewer/A2J_Shared": {
        "format": "global",
        "eval": "script",
        "deps": [
          "jquery-ui/ui/widgets/dialog"
        ]
      },
      "a2jauthor/legacy/viewer/A2J_SharedSus": {
        "format": "global",
        "eval": "script"
      },
      "a2jauthor/legacy/viewer/A2J_AnswerFile": {
        "format": "global",
        "eval": "script",
        "deps": [
          "a2jauthor/legacy/viewer/A2J_Types"
        ]
      },
      "a2jauthor/legacy/viewer/jquery.ui.combobox": {
        "deps": [
          "jquery-ui/ui/widget"
        ]
      },
      "a2jauthor/legacy/viewer/jquery.ui.traggable": {
        "deps": [
          "jquery-ui/ui/widgets/mouse",
          "jquery-ui/ui/widget"
        ]
      },
      "blueimp-file-upload/js/jquery.fileupload": {
        "deps": [
          "jquery"
        ]
      },
      "typeahead.js/dist/typeahead.jquery": {
        "format": "global"
      },
      "typeahead.js/dist/bloodhound": {
        "format": "cjs"
      },
      "bootstrap/js/modal": {
        "format": "global",
        "deps": [
          "jquery@2.1.4#dist/jquery"
        ],
        "globals": {
          "jQuery": "jquery@2.1.4#dist/jquery"
        }
      }
    },
    "paths": {
      "tokenfield-jquery": "node_modules/bootstrap-tokenfield/node_modules/jquery/dist/jquery.js"
    }
  },
  "dependencies": {
    "@caliorg/a2jdeps": "^7.1.0",
    "@caliorg/a2jviewer": "^7.2.4-0",
    "bit-tabs": "^2.0.0",
    "blueimp-file-upload": "^9.10.1",
    "bootstrap": "^3.4.1",
    "can-3-4-compat": "^1.0.0",
    "can-ajax": "^1.3.0",
    "can-component": "^4.2.4",
    "can-compute": "^4.1.0",
    "can-connect": "^2.1.0",
    "can-construct": "^3.5.0",
    "can-construct-super": "^3.2.0",
    "can-control": "^4.2.0",
    "can-define": "^2.8.0",
    "can-dom-data": "^1.0.3",
    "can-dom-events": "^1.3.0",
    "can-event-queue": "^1.1.7",
    "can-globals": "^1.2.0",
    "can-key": "^1.2.0",
    "can-list": "^4.2.0",
    "can-map": "^4.3.12",
    "can-map-backup": "^4.0.0",
    "can-map-compat": "^1.1.0",
    "can-map-define": "^4.4.0",
    "can-model": "^4.0.3",
    "can-observation": "^4.1.0",
    "can-observation-recorder": "^1.2.0",
    "can-reflect": "^1.17.0",
    "can-reflect-promise": "^2.1.0",
    "can-route": "^4.3.5",
    "can-route-pushstate": "^4.1.1",
    "can-stache": "^4.17.21",
    "can-stache-converters": "^4.2.5",
    "can-stache-key": "^1.4.0",
    "can-stache-route-helpers": "^1.1.1",
    "can-super-model": "^1.0.1",
    "can-util": "^3.12.0",
    "can-validate-legacy": "^2.0.0",
    "can-value": "^1.1.0",
    "can-view-autorender": "^4.2.0",
    "can-view-callbacks": "^4.4.1",
    "can-view-import": "^4.2.0",
    "connect": "^3.7.0",
    "cuid": "^2.1.6",
    "http-server": "^0.10.0",
    "jointjs": "^3.2.0",
    "jquery": "^3.4.1",
    "jquery-ui": "^1.12.1",
    "jquerypp": "^2.0.2",
    "lightbox2": "^2.10.0",
    "localforage": "^1.3.0",
    "lodash": "^4.17.15",
    "moment": "^2.10.3",
    "numbro": "^2.1.0",
    "numeral": "^1.5.6",
    "path-browserify": "0.0.0",
    "pdfjs-dist": "^2.0.466",
    "string-natural-compare": "^2.0.2",
    "text-statistics": "^0.1.1",
    "typeahead.js": "git://github.com/twitter/typeahead.js.git#v0.11.1",
    "validate.js": "^0.12.0"
  },
  "devDependencies": {
    "can-debug": "^2.0.1",
    "can-fixture": "^2.1.0",
    "can-view-model": "^4.0.3",
    "chai": "^4.1.2",
    "funcunit": "^3.0.0",
    "grunt": "^1.0.1",
    "grunt-contrib-clean": "^1.1.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-jshint": "^1.1.0",
    "grunt-contrib-less": "^2.0.0",
    "grunt-run": "^0.8.0",
    "grunt-steal": "^1.0.0",
    "sinon": "^1.17.2",
    "standard": "^11.0.1",
    "steal": "^1.12.4",
    "steal-css": "^1.3.1",
    "steal-less": "^1.2.2",
    "steal-mocha": "^2.0.1",
    "steal-stache": "^4.1.5",
    "steal-tools": "^1.11.10",
    "testee": "^0.9.1"
  }
}
