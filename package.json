{
  "name": "caja",
  "version": "0.0.1",
  "main": "server.stache!done-autorender",
  "description": "Authoring and viewer App GUI.",
  "scripts": {
    "test": "npm run mocha",
    "build": "babel src --out-dir lib",
    "build:server": "steal-tools build",
    "prestart": "npm run build && npm run build:server",
    "start": "NODE_ENV=production node ./bin/www",
    "dev": "npm run build && node ./bin/www",
    "build:viewer-zip": "node ./bin/make-viewer-package",
    "build:client": "grunt build --gruntfile=js/Gruntfile.js",
    "mocha": "mocha test/routes test/util --compilers js:babel-register",
    "mocha:watch": "mocha test/routes test/util --compilers js:babel-register --watch"
  },
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/CCALI/CAJA"
  },
  "system": {
    "directories": {
      "lib": "js"
    },
    "map": {
      "bloodhound": "typeahead.js/dist/bloodhound",
      "cuid": "cuid/dist/browser-cuid",
      "typeahead": "typeahead.js/dist/typeahead.jquery"
    },
    "npmIgnore": [
      "documentjs",
      "testee",
      "donejs-deploy",
      "generator-donejs",
      "donejs-cli",
      "babel-cli"
    ]
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "bootstrap": "3.3.5",
    "bootstrap-tokenfield": "^0.12.0",
    "can": "^2.3.0",
    "can-ssr": "^0.11.6",
    "cookie-parser": "~1.3.5",
    "cuid": "^1.3.8",
    "del": "^2.2.0",
    "done-autorender": "^0.6.0-pre.0",
    "done-css": "^1.1.15",
    "feathers": "^1.3.0",
    "filenamify": "^1.2.0",
    "fs-extra": "^0.26.5",
    "he": "^0.5.0",
    "helmet": "^1.1.0",
    "jade": "~1.11.0",
    "jquery": "~2.1.4",
    "lodash": "^4.0.0",
    "moment": "^2.10.3",
    "morgan": "~1.6.1",
    "q": "^1.4.1",
    "request": "^2.69.0",
    "serve-favicon": "~2.3.0",
    "steal": "^0.12.4",
    "typeahead.js": "git://github.com/twitter/typeahead.js.git#v0.11.1",
    "url-regex": "^3.1.0",
    "wkhtmltopdf": "^0.1.5"
  },
  "devDependencies": {
    "archiver": "^0.21.0",
    "assert": "^1.3.0",
    "babel-cli": "^6.4.5",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.4.3",
    "chalk": "^1.1.1",
    "debug": "^2.2.0",
    "fs-extra": "https://registry.npmjs.org/fs-extra/-/fs-extra-0.26.5.tgz",
    "grunt": "^0.4.5",
    "grunt-cli": "^0.1.13",
    "mocha": "^2.3.4",
    "sinon": "^1.17.2",
    "steal-tools": "^0.12.1",
    "supertest": "^1.2.0"
  }
}
